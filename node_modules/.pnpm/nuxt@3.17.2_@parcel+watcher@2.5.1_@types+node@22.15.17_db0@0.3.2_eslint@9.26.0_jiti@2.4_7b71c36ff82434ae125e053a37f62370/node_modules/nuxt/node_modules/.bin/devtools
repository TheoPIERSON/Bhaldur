#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/@nuxt+devtools@2.4.0_vite@6.3.5_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13_typescript@5.8.3_/node_modules/@nuxt/devtools/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/@nuxt+devtools@2.4.0_vite@6.3.5_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13_typescript@5.8.3_/node_modules/@nuxt/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/@nuxt+devtools@2.4.0_vite@6.3.5_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13_typescript@5.8.3_/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/@nuxt+devtools@2.4.0_vite@6.3.5_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13_typescript@5.8.3_/node_modules/@nuxt/devtools/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/@nuxt+devtools@2.4.0_vite@6.3.5_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13_typescript@5.8.3_/node_modules/@nuxt/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/@nuxt+devtools@2.4.0_vite@6.3.5_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13_typescript@5.8.3_/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@nuxt+devtools@2.4.0_vite@6.3.5_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13_typescript@5.8.3_/node_modules/@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../@nuxt+devtools@2.4.0_vite@6.3.5_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13_typescript@5.8.3_/node_modules/@nuxt/devtools/cli.mjs" "$@"
fi
