#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/nuxt@3.17.2_@parcel+watcher@2.5.1_@types+node@22.15.17_db0@0.3.2_eslint@9.26.0_jiti@2.4_7b71c36ff82434ae125e053a37f62370/node_modules/nuxt/bin/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/nuxt@3.17.2_@parcel+watcher@2.5.1_@types+node@22.15.17_db0@0.3.2_eslint@9.26.0_jiti@2.4_7b71c36ff82434ae125e053a37f62370/node_modules/nuxt/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/nuxt@3.17.2_@parcel+watcher@2.5.1_@types+node@22.15.17_db0@0.3.2_eslint@9.26.0_jiti@2.4_7b71c36ff82434ae125e053a37f62370/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/nuxt@3.17.2_@parcel+watcher@2.5.1_@types+node@22.15.17_db0@0.3.2_eslint@9.26.0_jiti@2.4_7b71c36ff82434ae125e053a37f62370/node_modules/nuxt/bin/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/nuxt@3.17.2_@parcel+watcher@2.5.1_@types+node@22.15.17_db0@0.3.2_eslint@9.26.0_jiti@2.4_7b71c36ff82434ae125e053a37f62370/node_modules/nuxt/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/nuxt@3.17.2_@parcel+watcher@2.5.1_@types+node@22.15.17_db0@0.3.2_eslint@9.26.0_jiti@2.4_7b71c36ff82434ae125e053a37f62370/node_modules:/Users/theopierson/Sites/lab/bhaldur/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../../bin/nuxt.mjs" "$@"
fi
